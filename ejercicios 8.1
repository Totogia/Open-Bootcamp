def ordene(fichero, datos):
    f = open(fichero, "w")
    
    for linea in datos:

        if not linea.endswith ("\n"):
            linea = linea + "\n"
        
        f.write(linea)
       

    f.close()

Lista = [
            "billetera",
            "piedra",
            "collar",
            "desodorante",
            "sordina",
                            ]



ordene ("mifichero.txt", Lista)
