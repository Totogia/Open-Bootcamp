import pickle
class veiculo:
    marca = ""
    modelo = 0
    cilindrada = 0.0
    puertas = 0

    def __init__(self, marca, modelo, cilindrada, puertas) :
        self.marca = marca
        self.modelo = modelo
        self.cilindrada = cilindrada
        self.puertas = puertas
    
    def getMarca (self):
        return self.marca

V1 = veiculo ("Fiat", 2014, 1.4, 3)

